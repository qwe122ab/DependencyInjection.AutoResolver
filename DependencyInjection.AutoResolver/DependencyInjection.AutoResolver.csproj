<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <Title>DependencyInjection.AutoResolver</Title>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Company></Company>
    <Authors>Tadas Scerbickas</Authors>
    <RepositoryUrl>https://github.com/tscerbickas/DependencyInjection.AutoResolver</RepositoryUrl>
    <PackageTags>DependencyInjection;AutoResolver</PackageTags>
    <PackageProjectUrl>https://www.nuget.org/packages/DependencyInjection.AutoResolver</PackageProjectUrl>
    <Version>1.0.3</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="6.0.0" />
  </ItemGroup>

</Project>
